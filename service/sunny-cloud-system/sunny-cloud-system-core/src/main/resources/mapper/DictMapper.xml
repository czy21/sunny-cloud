<mapper namespace="com.sunny.cloud.system.core.mapper.DictPOMapper">
    <insert id="insert" keyColumn="id" keyProperty="id" parameterType="com.sunny.cloud.system.core.model.po.DictPO" useGeneratedKeys="true">
        insert into dict
        <trim prefix="(" suffix=")" suffixOverrides=",">
                <if test="code != null">code,</if>
                <if test="name != null">name,</if>
                <if test="desc != null">desc,</if>
                <if test="valueType != null">value_type,</if>
                <if test="createTime != null">create_time,</if>
                <if test="createUser != null">create_user,</if>
                <if test="updateTime != null">update_time,</if>
                <if test="updateUser != null">update_user,</if>
                <if test="deleted != null">deleted,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
                <if test="code != null">#{code,jdbcType=VARCHAR},</if>
                <if test="name != null">#{name,jdbcType=VARCHAR},</if>
                <if test="desc != null">#{desc,jdbcType=VARCHAR},</if>
                <if test="valueType != null">#{valueType,jdbcType=TINYINT},</if>
                <if test="createTime != null">#{createTime,jdbcType=TIMESTAMP},</if>
                <if test="createUser != null">#{createUser,jdbcType=VARCHAR},</if>
                <if test="updateTime != null">#{updateTime,jdbcType=TIMESTAMP},</if>
                <if test="updateUser != null">#{updateUser,jdbcType=VARCHAR},</if>
                <if test="deleted != null">#{deleted,jdbcType=BIT},</if>
        </trim>
    </insert>
    <update id="update" parameterType="com.sunny.cloud.system.core.model.po.DictPO">
        update dict
        <set>
                <if test="code != null">
                    code = #{code,jdbcType=VARCHAR},
                </if>
                <if test="name != null">
                    name = #{name,jdbcType=VARCHAR},
                </if>
                <if test="desc != null">
                    desc = #{desc,jdbcType=VARCHAR},
                </if>
                <if test="valueType != null">
                    value_type = #{valueType,jdbcType=TINYINT},
                </if>
                <if test="createTime != null">
                    create_time = #{createTime,jdbcType=TIMESTAMP},
                </if>
                <if test="createUser != null">
                    create_user = #{createUser,jdbcType=VARCHAR},
                </if>
                <if test="updateTime != null">
                    update_time = #{updateTime,jdbcType=TIMESTAMP},
                </if>
                <if test="updateUser != null">
                    update_user = #{updateUser,jdbcType=VARCHAR},
                </if>
                <if test="deleted != null">
                    deleted = #{deleted,jdbcType=BIT},
                </if>
        </set>
        where   id = #{id,jdbcType=BIGINT} 
    </update>
</mapper>
