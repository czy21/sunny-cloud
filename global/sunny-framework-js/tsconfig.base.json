{
  "compilerOptions": {
    "allowSyntheticDefaultImports": false,
    "esModuleInterop": false,
    "lib": [
      "es6"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmit": false,
    "noEmitOnError": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "pretty": false,
    "sourceMap": false,
    "strict": false,
    "target": "es2019"
  },
  "exclude": ["dist","node_modules","test/types"]
}